CREATE TABLE leandro_mar_b_coderhouse.pokemon_table(
    id INT PRIMARY KEY,
    nombre VARCHAR(50),
    peso DECIMAL(10,2),
    altura DECIMAL(10,2),
    experiencia DECIMAL(10,2),
    tipos VARCHAR(100),
    habilidades VARCHAR(100),
    hp DECIMAL(10,2),
    ataque DECIMAL(10,2),
    defensa DECIMAL(10,2),
    ataque_especial DECIMAL(10,2),
    defensa_especial DECIMAL(10,2),
    velocidad DECIMAL(10,2),
    movimientos VARCHAR(100)
);
